{
    "metadata": {
        "title": "citruspi"
    },
    "ignore": [
        ".*\\..+\\.j2.json",
        "\\./public.*"
    ],
    "processors": {
        "(.*\\..+)\\.j2": {
            "processor": "jinja"
        },
        "(.*\\..+)\\.sh": {
            "processor": "shell",
            "command": "./process.sh {path} {public_path}"
        }
    }
}
