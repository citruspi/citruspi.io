<!DOCTYPE html>
<html lang="en">
    <head>
        {% block head %}
        <meta charset="utf-8">

        <title>citruspi | {{ title }}</title>

        <meta name="viewport" content="width=device-width, initial-scale=1">
        {% endblock %}

        {% block externalstylesheets %}
        <link rel="stylesheet" href="/static/css/normalize.css">
        <link rel="stylesheet" href="/static/css/skeleton.css">
        <link rel="stylesheet" href="/static/css/solarized_dark.css">
        {% endblock %}
        {% block internalstylesheets %}
        <link rel="stylesheet" href="/static/css/moonshine.css">
        {% endblock %}
    </head>
    <body>
        {% block body %}
        {% endblock %}

        {% block externalscripts %}
        <script src="https://use.typekit.net/ypc6utl.js"></script>
        <script>try{Typekit.load({ async: true });}catch(e){}</script>
        {% endblock %}
        {% block internalscripts %}
        <script src="/static/js/navigation.js"></script>
        {% endblock %}
    </body>
</html>
